<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN" "http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd">
<taglib>
<tlibversion>1.0</tlibversion>
<jspversion>1.1</jspversion>
<shortname>paginator</shortname>
<uri></uri>
<info>Tab Library for PaginatorTag</info>
<tag>
    <name>paginator</name>
    <tagclass>com.gmcc.boss.selfsvc.query.tag.NewPaginatorTag</tagclass>
    <bodycontent>JSP</bodycontent>
    <info>Returns a paginator</info>
    <attribute>
       <name>recordsCount</name>
       <required>true</required>
       <rtexprvalue>true</rtexprvalue>
    </attribute>    
    
    <attribute>
       <name>pageSize</name>
       <required>true</required>
       <rtexprvalue>true</rtexprvalue>
    </attribute>

    <attribute>
       <name>page</name>
       <required>false</required>
       <rtexprvalue>true</rtexprvalue>
    </attribute>

    <attribute>
       <name>linkUrl</name>
       <required>true</required>
       <rtexprvalue>true</rtexprvalue>
    </attribute>    
    
    <attribute>
       <name>alignType</name>
       <required>false</required>
       <rtexprvalue>false</rtexprvalue>
    </attribute>    
    
    <attribute>
       <name>cdrFlag</name>
       <required>false</required>
       <rtexprvalue>false</rtexprvalue>
    </attribute>    
    
    <attribute>
       <name>isQueryAll</name>
       <required>false</required>
       <rtexprvalue>true</rtexprvalue>
    </attribute>
    
    <attribute>
       <name>listtype</name>
       <required>false</required>
       <rtexprvalue>true</rtexprvalue>
    </attribute>
</tag>
</taglib>
