package com.gmcc.boss.selfsvc.login.model;

import com.gmcc.boss.selfsvc.common.BasePO;

/**
 * 
 * 错误的登录记录
 * 
 * 
 * @author g00140516
 * @version 1.0，2010-12-20
 * @see
 * @since
 */
public class LoginErrorPO extends BasePO
{
    private static final long serialVersionUID = -9066580054390068166L;
    
    /**
     * 手机号码
     */
    private String servNumber = "";
    
    /**
     * 认证方式
     */
    private String authType = "";
    
    /**
     * 错误次数
     */
    private int errorTimes = 0;
    
    /**
     * 第一次认证失败的时间
     */
    private String firstTime = "";
    
    /**
     * 最后一次认证失败的时间
     */
    private String lastTime = "";
    
    public String getServNumber()
    {
        return servNumber;
    }
    
    public void setServNumber(String servNumber)
    {
        this.servNumber = servNumber;
    }
    
    public String getAuthType()
    {
        return authType;
    }
    
    public void setAuthType(String authType)
    {
        this.authType = authType;
    }
    
    public int getErrorTimes()
    {
        return errorTimes;
    }
    
    public void setErrorTimes(int errorTimes)
    {
        this.errorTimes = errorTimes;
    }
    
    public String getFirstTime()
    {
        return firstTime;
    }
    
    public void setFirstTime(String firstTime)
    {
        this.firstTime = firstTime;
    }
    
    public String getLastTime()
    {
        return lastTime;
    }
    
    public void setLastTime(String lastTime)
    {
        this.lastTime = lastTime;
    }
}
