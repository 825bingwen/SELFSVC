<script type = "text/javascript">
	if("1" == <%=usimSwitch %>)
	{
		setTimeout("checkUSIMCard()",500);
		function checkUSIMCard()
		{
			// 若第一次登陆、且省份为山东、人员登录信息非空
			if ("1" == "<%=usimFlag %>" && '<%=province%>' == '<%=provinceSDInfo%>'
				&& "null" != '<%=customerInfor%>')
			{
				var url = "${sessionScope.basePath}usim/checkUSIM.action";
				var param = "servnumber=" + '<%=telnum%>'
						+"&curMenuId=" + '<%=curMenuId%>';
				var data = "";
				var contentLoader=new net.ContentLoaderSynchro(url,function()
				{
					// 将USIM卡标志定位1
					// document.getElementById("usimTimes").value = '1';
					data = this.req.responseText;
					
				// 若未更换USIM卡
				if (data == "unChange")
				{
					alertSuccessMsg("<%=alertMessage%>");
				}
				},null,"POST",param,"application/x-www-form-urlencoded");
			}
		}
	}
</script>
