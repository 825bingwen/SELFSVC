public class AwardVO
{
    private String rewardId;// 奖品编码
    
    private String rewardName;// 奖品名称
    
    private String rewardType;// 奖品类型
    
    private String rewardTypeName;// 奖品类型名称
    
    private String rewardValue;// 奖品价值
    
    private String scoreDealType;// 积分扣减类型
    
    private String scoreDealTypeName;// 积分扣减类型名称
    
    private String userScore;// 扣减积分数额
    
    private String rewardNote;// 奖品说明

    public String getRewardId()
    {
        return rewardId;
    }

    public void setRewardId(String rewardId)
    {
        this.rewardId = rewardId;
    }

    public String getRewardName()
    {
        return rewardName;
    }

    public void setRewardName(String rewardName)
    {
        this.rewardName = rewardName;
    }

    public String getRewardType()
    {
        return rewardType;
    }

    public void setRewardType(String rewardType)
    {
        this.rewardType = rewardType;
    }

    public String getRewardValue()
    {
        return rewardValue;
    }

    public void setRewardValue(String rewardValue)
    {
        this.rewardValue = rewardValue;
    }

    public String getScoreDealType()
    {
        return scoreDealType;
    }

    public void setScoreDealType(String scoreDealType)
    {
        this.scoreDealType = scoreDealType;
    }

    public String getRewardNote()
    {
        return rewardNote;
    }

    public void setRewardNote(String rewardNote)
    {
        this.rewardNote = rewardNote;
    }

    public String getUserScore()
    {
        return userScore;
    }

    public void setUserScore(String userScore)
    {
        this.userScore = userScore;
    }

    public String getRewardTypeName()
    {
        return rewardTypeName;
    }

    public void setRewardTypeName(String rewardTypeName)
    {
        this.rewardTypeName = rewardTypeName;
    }

    public String getScoreDealTypeName()
    {
        return scoreDealTypeName;
    }

    public void setScoreDealTypeName(String scoreDealTypeName)
    {
        this.scoreDealTypeName = scoreDealTypeName;
    }



    
    
}
