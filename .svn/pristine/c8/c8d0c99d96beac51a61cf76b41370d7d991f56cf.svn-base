	<sqlMap resource="ibatis/sql-map-base.xml"/>
	<sqlMap resource="ibatis/sql-map-resData.xml"/>
	<sqlMap resource="ibatis/sql-map-terminalInfo.xml"/>
	<sqlMap resource="ibatis/sql-map-media.xml"/>
	<sqlMap resource="ibatis/sql-map-login.xml"/>
	<sqlMap resource="ibatis/sql-map-feeservice.xml"/>
	<sqlMap resource="ibatis/sql-map-charge.xml"/>
	<sqlMap resource="ibatis/sql-map-managerOperation.xml"/>
	<sqlMap resource="ibatis/sql-map-spService.xml"/>
	<sqlMap resource="ibatis/sql-map-quickpublish.xml"/>
	
	<sqlMap resource="ibatis/customize/hub/sql-map-chooseTel.xml"/>
	<sqlMap resource="ibatis/customize/hub/sql-map-privAccept.xml"/>
	<sqlMap resource="ibatis/customize/hub/sql-map-prodChange.xml"/>
	<sqlMap resource="ibatis/customize/hub/sql-map-charge-hub.xml"/>
	<sqlMap resource="ibatis/customize/hub/sql-map-menu-hub.xml"/>
	<sqlMap resource="ibatis/customize/hub/sql-map-activities.xml"/>
